<!-- allStudents.ejs -->

<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>All Students</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    </head>

    <body class="bg-body-secondary">
        <%- include('../partials/flashPartials.ejs') %>
        <div class="container p-4">


            <table class="table table-hover table-warning mt-3">
                <thead>
                    <tr>
                        <th scope="col">Sr.No</th>
                        <th scope="col">Full Name</th>
                        <th scope="col">PRN</th>
                        <th scope="col">BRANCH</th>
                        <th scope="col">Placement Status</th>
                        <th scope="col">Year</th>
                        <th scope="col">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% students.forEach((s,index)=>{ %>
                    <tr>
                        <th scope="row"><%= index+1 %></th>
                        <td><%= s.personalDetails.fullName %></td>
                        <td><%= s.personalDetails.PRN %></td>
                        <td><%= s.personalDetails.branch %></td>
                        <td><%= s.personalDetails.placementStatus %></td>
                        <td><%= s.personalDetails.year %></td>
                        <td>
                            <a href="/profile/<%= s._id %>" class="btn btn-sm btn-info">profile</a>
                        </td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
            crossorigin="anonymous"></script>
    </body>

</html>